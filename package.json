{
  "name": "extlib",
  "description": "Complementary extra standard library for JS/TS; type safe, no dependencies, modular imports",
  "version": "0.2.0",
  "main": "js/main.js",
  "files": [
    "js/**"
  ],
  "scripts": {
    "build": "npm run clean && tsc && find bin/postbuild -type f | parallel '{}' \"$(realpath js)\"",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register src/test/**/*.spec.ts",
    "prepublishOnly": "npm run test && npm run build",
    "clean": "rm -rf js"
  },
